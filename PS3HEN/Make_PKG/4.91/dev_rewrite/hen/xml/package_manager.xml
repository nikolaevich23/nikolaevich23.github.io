<XMBML version="1.0">
	<View id="seg_package_files">
		<Attributes>
			<Table key="package_files">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="icon_notation"><String>WNT_XmbItemAlbum</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_package_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_package_file_info</String></Pair>
			</Table>
		</Attributes>
		<Items>
			<Query  class="type:x-xmb/folder-pixmap"  key="package_files" attr="package_files" src="#seg_package_fixed"/>
		</Items></View>
	
	<View id="seg_package_fixed">
		<Attributes>
			<Table key="package_files_install">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_install_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_install_file_info</String></Pair>
			</Table>
			<Table key="host_psn">
				<Pair key="icon_rsc"><String>item_tex_online_storage</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_psn_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_psn_file_info</String></Pair>
				<Pair key="str_noitem"><String>msg_tool_no_psn_content</String></Pair>
				<Pair key="child"><String>segment</String></Pair>
			</Table>			
			<Table key="seg_package_files_delete">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_delete_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_delete_file_info</String></Pair>
			</Table>
		</Attributes>
		<Items>
			<Query class="type:x-xmb/folder-pixmap" key="package_files_install" attr="package_files_install" src="#seg_install_package"/>
			<Query class="type:x-xmb/folder-pixmap" key="host_psn" attr="host_psn" src="#seg_install_files_psn_path"/>
			<Query class="type:x-xmb/folder-pixmap" key="seg_package_files_delete" attr="seg_package_files_delete" src="#seg_delete_package"/>
			<!--<Query class="type:x-xmb/folder-pixmap" key="mg" attr="mg" src="#mount_game"/>-->
		</Items></View>
		
	<View id="mount_game">
		<Attributes>
			<Table key="mount_items">
				<Pair key="icon_rsc"><String>item_tex_disc_install</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_mount</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_mount_info</String></Pair>
				<Pair key="module_name"><String>xai_plugin</String></Pair>
				<Pair key="module_action"><String>mount_hdd_game</String></Pair>
				<Pair key="bar_action"><String>none</String></Pair>
				<Pair key="lbl_half"><String>1</String></Pair>
			</Table>
			<Table key="unmount_items">
				<Pair key="icon_rsc"><String>item_tex_disc_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_unmount</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_unmount_info</String></Pair>
				<Pair key="module_name"><String>xai_plugin</String></Pair>
				<Pair key="module_action"><String>unmount_hdd</String></Pair>
				<Pair key="bar_action"><String>none</String></Pair>
				<Pair key="lbl_half"><String>1</String></Pair>
			</Table>
		</Attributes>
		<Items>
			<Item class="type:x-xmb/module-action"  key="mount_items" attr="mount_items" />
			<Item class="type:x-xmb/module-action"  key="unmount_items" attr="unmount_items" />
		</Items>
	</View>

	<View id="seg_install_files_psn_path">
		<Items>
			<Query class="type:x-xmb/folder-pixmap" key="seg_hdd_install" src="ese://localhost/query?type=install_game"/>
			<Query class="type:x-xmb/folder-pixmap" key="seg_pkg_install" src="ese://localhost/query?type=install_pkg"/>
		</Items></View>
	
	<View id="seg_install_package">
		<Attributes>
			<Table key="host_device">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="icon_notation"><String>WNT_XmbItemAlbum</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_all_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_all_info</String></Pair>
				<Pair key="str_noitem"><String>msg_error_no_content</String></Pair>
				<Pair key="child"><String>segment</String></Pair>
				<Pair key="ingame"><String>disable</String></Pair>
			</Table>
			<Table key="host_device_1">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_hdd0_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_hdd0_install_info</String></Pair>
				<Pair key="str_noitem"><String>msg_tool_no_package</String></Pair>
				<Pair key="child"><String>segment</String></Pair>
			</Table>
			<Table key="host_device_2">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_usb_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_usb_install_info</String></Pair>
				<Pair key="str_noitem"><String>msg_tool_no_package</String></Pair>
				<Pair key="child"><String>segment</String></Pair>
			</Table>
			<Table key="host_device_3">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_og_file</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_og_install_info</String></Pair>
				<Pair key="str_noitem"><String>msg_tool_no_package</String></Pair>
				<Pair key="child"><String>segment</String></Pair>
			</Table>
			<Table key="pkg_linker">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title_rsc"><String>msg_tool_lw</String></Pair>
				<Pair key="info_rsc"><String>msg_tool_lw_info</String></Pair>
				<Pair key="str_noitem"><String>msg_tool_no_package</String></Pair>
			</Table>			
		</Attributes>
		<Items>
			<Query class="type:x-xmb/folder-pixmap" key="host_device" attr="host_device" src="#seg_install_files_alldev_path"/>
			<Query class="type:x-xmb/folder-pixmap" key="host_device_1" attr="host_device_1" src="#seg_install_files_hdd0_path"/>
			<Query class="type:x-xmb/folder-pixmap" key="host_device_2" attr="host_device_2" src="#seg_install_usb_path"/>
			<Query class="type:x-xmb/xmlpackagefolder" key="host_device_3" attr="host_device_3" src="#seg_install_files_og_path"/>
			<Query class="type:x-xmb/folder-pixmap" key="pkg_linker"  attr="pkg_linker" src="xmb://localhost/dev_hdd0/game/PKGLINKER/USRDIR/package_link.xml#pkg_items"/>
		</Items>
	</View>
	
